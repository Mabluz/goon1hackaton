{
  "apiVersion" : "v1",
  "kind" : "DeploymentConfig",
  "metadata" : {
    "annotations" : {
      "openshift.io/generated-by" : "DCHUB Occam",
      "dchub.occam/spec.parent" : "/dchub/node-v8-spec.json",
      "dchub.occam/spec.build.parent" : "/dchub/build-spec/node-v8.json",
      "dchub.occam/spec.deployment.parent" : "/dchub/deployment-spec/node.json"
    },
    "labels" : {
      "app" : "component-tracking"
    },
    "name" : "component-tracking",
    "namespace" : "web-services"
  },
  "spec" : {
    "replicas" : 2,
    "selector" : {
      "app" : "component-tracking",
      "deploymentconfig" : "component-tracking"
    },
    "strategy" : {
      "resources" : { },
      "type" : "Rolling"
    },
    "template" : {
      "metadata" : {
        "annotations" : {
          "openshift.io/generated-by" : "DCHUB Occam"
        },
        "labels" : {
          "app" : "component-tracking",
          "deploymentconfig" : "component-tracking"
        }
      },
      "spec" : {
        "serviceAccount" : "default",
        "serviceAccountName" : "default",
        "containers" : [ {
          "image" : "web-services/component-tracking:prod",
          "env" : [ {
            "name" : "TZ",
            "value" : "Europe/Oslo"
          }, {
            "name" : "LANG",
            "value" : "nb_NO.UTF-8"
          }, {
            "name" : "NODE_ENV",
            "value" : "production"
          }, {
            "name" : "PORT",
            "value" : "8080"
          } ],
          "name" : "component-tracking",
          "ports" : [ {
            "containerPort" : 8080,
            "protocol" : "TCP"
          } ],
          "resources" : {
            "limits" : {
              "memory" : "768Mi",
              "cpu" : "300m"
            },
            "requests" : {
              "memory" : "512Mi",
              "cpu" : "100m"
            }
          },
          "volumeMounts" : [ ],
          "lifecycle" : {
            "preStop" : {
              "exec" : {
                "command" : [ "sleep", "5" ]
              }
            }
          }
        } ],
        "volumes" : [ ]
      }
    },
    "triggers" : [ {
      "type" : "ConfigChange"
    }, {
      "imageChangeParams" : {
        "automatic" : true,
        "containerNames" : [ "component-tracking" ],
        "from" : {
          "kind" : "ImageStreamTag",
          "name" : "component-tracking:prod",
          "namespace" : "web-services"
        }
      },
      "type" : "ImageChange"
    } ]
  }
}